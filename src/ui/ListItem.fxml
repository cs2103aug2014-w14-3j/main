<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hbox" prefHeight="20.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="desc" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="400">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="1.0" left="10.0" right="5.0" top="2.0" />
         </HBox.margin>
      </Text>
      <TextFlow fx:id="time" prefHeight="0.0" prefWidth="150.0" textAlignment="RIGHT">
         <HBox.margin>
            <Insets left="5.0" right="10.0" />
         </HBox.margin>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TextFlow>
   </children>
</HBox>
