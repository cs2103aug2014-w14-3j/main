                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time" textAlignment="RIGHT">
                           <font>
                              <Font name="Tahoma Bold" size="16.0" />
                           </font>
                        </Text>
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </TextFlow>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="0.5" left="0.5" right="0.5" top="0.5" />
